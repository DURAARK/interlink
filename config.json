    "vocabularies": {
        "aat": {
            "prefix": "http://vocab.getty.edu/aat/",
            "endpoint": "http://vocab.getty.edu/sparql.{fmt}?query={query}&_implicit=false&implicit=true&_equivalent=false&_form=%2Fsparql",
            "predicates": [
                "rdfs:label",
                "rdfs:subClassOf",
                "gvp:broader",
                "gvp:narrower",
                "skos:broader",
                "skos:prefLabel"        
            ]
        },
        "wordnet": {
            "prefix": "http://wordnet-rdf.princeton.edu/wn31/",
            "endpoint": "http://bw-dssv19.bwk.tue.nl:8890/sparql?default-graph-uri=&query={query}&format={fmt}",
            "predicates": [
                "wordn-sc:hyponymOf",
                "wordn-sc:gloss",
                "wn:gloss",
                "wn:hyponym",
                "wn:hypernym",
                "rdfs:label",
                "lemon:reference"
            ]
        },
        "bsdd": {
            "prefix": "bsdd:",
            "endpoint": "http://bw-dssv16.bwk.tue.nl:8080/openrdf-workbench/repositories/bsdd/query?action=exec&queryLn=SPARQL&query={query}",
            "accept": "application/json"
        },
        "tgn": {
            "prefix": "http://vocab.getty.edu/tgn/",
            "endpoint": "http://vocab.getty.edu/sparql.{fmt}?query={query}&_implicit=false&implicit=true&_equivalent=false&_form=%2Fsparql"
        },
        "dbpedia": {
            "prefix": "http://dbpedia.org/resource/",
            "endpoint": "http://dbpedia.org/sparql?default-graph-uri=http://dbpedia.org&query={query}&format=application/json",
            "predicates": [
                "rdfs:label",
                "dc-term:subject",
                "skos:broader",
                "dbp-ont:wikiPageDisambiguates"
            ]
        }
    }
}